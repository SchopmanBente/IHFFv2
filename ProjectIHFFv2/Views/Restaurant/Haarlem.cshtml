@using ProjectIHFFv2.Models;

@model IEnumerable<Event>




<h2>Haarlem</h2>

@foreach (Event r in Model)
{


    <h3> @r.naam</h3>

    <img width="100" height="120" src="@r.afbeelding_url" />
        <p> @r.beschrijving</p>
        @Html.ActionLink("More info", "DetailPagina", "Restaurant", new { id = @r.EventId }, new { @class = "btn btn-info" })
}
